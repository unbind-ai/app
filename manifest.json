{
  "manifest_version": 3,
  "name": "Unbind â€” Export Your AI Conversations",
  "version": "1.0.0",
  "description": "Export your ChatGPT conversations with one click. Works with Free, Plus, and Teams accounts.",
  "author": "Unbind",
  "homepage_url": "https://unbind-ai.github.io/app",
  
  "permissions": [
    "storage",
    "downloads"
  ],
  
  "host_permissions": [
    "https://chatgpt.com/*",
    "https://chat.openai.com/*"
  ],
  
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://chat.openai.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  
  "background": {
    "service_worker": "background.js"
  }
}
